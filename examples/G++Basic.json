{
	"title": "Compile CPP",
	"version": 2, 
	"exec": "g++",
	"args": {
		"source": {
			"type": "fmtstr",
			"format": "%s",
            "prompt": "Source File"
		},
		"save": {
			"type": "fmtstr",
			"format": "-o %s",
            "prompt": "Save As"
		},
		"std": {
			"type": "choose",
			"options":["14", "11", "98"],
			"default":"11",
			"format": "-std=c++%s",
            "prompt": "CPP Standard"
		},
		"O2": {
			"type": "str",
			"value": "-O2",
            "prompt": "O2"
		},
        "DisableANSI": {
            "type": "str",
            "value": "-ansi",
			"prompt": "Disable ANSI"
        }
	}
}