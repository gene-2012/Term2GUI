{
    "title": "Compile CPP",
    "version": 2,
    "exec": "g++",
    "args": {
        "source": {
            "type": "file",
            "method": "open", 
            "extensions": [".cpp", ".c", ".h"],
            "prompt": "Source File"
        },
        "save": {
            "extensions": [".exe"],
            "type": "file",
            "method": "save",
            "format": "-o %s",
            "prompt": "Save As"
        },
        "std": {
            "type": "choose",
            "options":["14", "11", "98"],
            "default":"11",
            "format": "-std=c++%s",
            "prompt": "CPP Standard"
        },
		"O2": {
			"type": "str",
			"value": "-O2",
            "prompt": "O2"
		},
        "DisableANSI": {
            "type": "str",
            "value": "-ansi",
			"prompt": "Disable ANSI"
        }
    }
}